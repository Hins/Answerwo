                    <li><input type="checkbox" /><label>选上不可用状态</label></li>
                    <li><input type="checkbox" /><label>警告的状态</label></li>
                </ul>
            </div>
        </form>
    </div>
    
    <div class="po-export Popup ym-clearfix" title="导出题目" popup-id="export" popup-btn="true" popup-width="470">
		<form class="fo-form">
        	<div class="item">
                <label class="title">导出格式<sup>*</sup></label>
                <div class="fo-dropdown ym-clearfix">
                    <select>
                        <option selected="selected">微软Office Word格式（*.docx）</option>
                        <option>Adobe PDF格式（*.pdf）</option>
                    </select>
                </div>
            </div>
            <div class="item">
                <label class="title">请选择要从哪里导出</label>
                <ul class="radio radio-aline ym-clearfix" id="ExportRadioShow">
                    <li radio-show="favorite"><input type="radio" name="test1" value="0" /><label>我的收藏夹</label></li>
                    <li radio-show="list"><input type="radio" name="test1" value="1" /><label>我的题目清单</label></li>
                </ul>
            </div>
            <div class="item" radio-target="favorite" style="display:none">
                <label class="title">请选择要导出的收藏夹标签</label>
                <div class="fo-dropdown ym-clearfix">
                    <select>
                        <option selected="selected">不导出收藏夹的题目</option>
                        <option>收藏夹标签1</option>
                        <option>Adobe PDF格式（*.pdf）</option>
                    </select>
                </div>
            </div>
            <div class="item" radio-target="list" style="display:none">
                <label class="title">请选择要导出的题目清单</label>
                <div class="fo-dropdown ym-clearfix">
                    <select>
                        <option selected="selected">不导出题目清单</option>
                        <option>题目清单名称</option>
                        <option>题目清单名称</option>
                    </select>
                </div>
            </div>
            <div class="item">
                <label class="title">要导出的内容<sup>*</sup></label>
                <ul class="check check-aline ym-clearfix">
                    <li><input type="checkbox" checked="checked" /><label>题目内容</label></li>
                    <li><input type="checkbox" checked="checked" /><label>题目答案</label></li>
                    <li><input type="checkbox" checked="checked" /><label>题目解析</label></li>
                    <li><input type="checkbox" checked="checked" /><label>个人批注</label></li>
                </ul>
            </div>
        </form>
    </div>
	
